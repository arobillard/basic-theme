{
  "title":"ThemeBasic",
  "meta":"ThemeBasic is a simple template for creating a website. With a number of styling and section options, ThemeBasic allows users to create beautiful, unique websites.",
  "owner":"Adam Robillard",
  "logos":{
    "type":"svg",
    "img": "https://placehold.it/150x50",
    "imgMobile": "https://placehold.it/100x100",
    "svg": "svg/logo.html",
    "widthMobile": "10rem"
  },
  "colors":{
    "primary":"#2c8Bd9",
    "primaryDark":"#1a5c93",
    "primaryLight":"#70ADDF",
    "secondary":"#DF4843",
    "secondaryDark":"#AB3733",
    "secondaryLight":"#E38B88",
    "grey":"#999",
    "greyDark":"#444",
    "greyLight":"#f2f2f2"
  },
  "fonts":{
    "primary":"'Montserrat', sans-serif",
    "secondary":"'Montserrat Alternates', sans-serif;"
  },
  "shadows":{
    "primary": "0 2px 3px rgba(50, 50, 50, .15)"
  },
  "transitions":{
    "primary": ".25s ease-in-out"
  },
  "header":{
    "headerStyle": "basic",
    "fixed": true
  },
  "nav":{
    "navItems":[
    ]
  },
  "sections":{
    "hero":{
      "height":"90vh",
      "bgOverlay": true,
      "bgOverlayColor": "greyDark",
      "bgOverlayOpacity": ".7",
      "contentAlignment":"left",
      "headlineColor":"primary",
      "bylineColor":"white",
      "btnStyle":"btn-ghost"
    },
    "cta":{
      "height":"50vh",
      "bgPosition":"center",
      "bgOverlay": true,
      "bgOverlayColor": "secondary",
      "bgOverlayOpacity": ".7",
      "contentAlignment":"center",
      "headlineColor":"primary",
      "bylineColor":"white",
      "btnStyle":"btn-ghost"
    }
  },
  "footer":{
    "footerStyle":"basic",
    "bgColor": "greyDark",
    "textColor": "grey",
    "linkColor": "grey",
    "linkHoverColor": "primary",
    "paddingExtra": false
  },
  "links":[
    {
      "name":"Github",
      "url":"#",
      "icon":"github"
    },
    {
      "name":"Instagram",
      "url":"#",
      "icon":"instagram"
    },
    {
      "name":"LinkedIn",
      "url":"#",
      "icon":"linkedin"
    },
    {
      "name":"Vimeo",
      "url":"#",
      "icon":"vimeo"
    }
  ]
}
